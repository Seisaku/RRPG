<?xml version="1.0" encoding="UTF-8"?>
<form name="frmBuff" height="20" margins="{top=2}">      
	<flowLayout align="client">
		<checkBox 	width="20" field="ativo"/>
		<edit 		width="80" height="20" field="nome"/>
		<edit 		width="120"  height="20" field="formula"/>
		<edit 		width="30"  height="20" type="number" min="-10" field="vantagem"/>
		<edit 		width="30"  height="20" type="number" min="0" field="turnos"/>
		<button 	width="20"  height="20" text="X" onClick="deleteBuff(sheet)" margins="{left=2}"/>	
	</flowLayout>
</form>